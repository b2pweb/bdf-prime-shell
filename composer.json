{
    "name": "b2pweb/bdf-prime-shell",
    "description": "Interactive shell for prime queries",
    "keywords": ["prime", "orm", "shell"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Vincent Quatrevieux",
            "email": "vquatrevieux@b2pweb.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Bdf\\Prime\\Shell\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bdf\\Prime\\Shell\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "require": {
        "php": "~7.1 || ~8.0.0 || ~8.1.0 || ~8.2.0 || ~8.3.0",
        "b2pweb/bdf-prime": "~1.0|~2.0",
        "b2pweb/bdf-prime-analyzer": "~1.0",
        "symfony/console": "~4.0 || ~5.0 || ~6.0",
        "psy/psysh": "~0.10",
        "b2pweb/bdf-collections": "~1.1",
        "b2pweb/bdf-util": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0|~8.0|~9.0",
        "vimeo/psalm": "~4.0|~5.22"
    },
    "scripts": {
        "tests": "phpunit",
        "tests-with-coverage": "phpunit --coverage-clover coverage.xml",
        "psalm": "psalm --shepherd --php-version=7.4"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
